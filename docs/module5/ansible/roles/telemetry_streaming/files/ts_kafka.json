{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-telemetry-streaming/v1.8.0/src/schema/1.8.0/base_schema.json",
    "schemaVersion": "1.8.0",
    "class": "Telemetry",
    "controls": {
        "class": "Controls",
        "logLevel": "info"
    },
    "My_Poller": {
        "class": "Telemetry_System_Poller",
        "interval": 60
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514
    },
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "Kafka",
        "host": "10.1.1.7",
        "protocol": "binaryTcp",
        "port": {{consumerPort}},
        "topic": "f5-telemetry"
    }
}